echo

start molecule

print low

title "Rn@Quartz ecp"
charge 0

geometry units angstroms print xyz autosym
  load format xyz RnQgeom.xyz
end

basis
 * library lanl2dz_ecp
end

ecp 
  Si library lanl2dz_ecp
  Hg library lanl2dz_ecp
end

dft
  xc b3lyp
  maxiter 100
end

driver
  maxiter 999
  xyz RnQgeomconv
end

task dft optimize

