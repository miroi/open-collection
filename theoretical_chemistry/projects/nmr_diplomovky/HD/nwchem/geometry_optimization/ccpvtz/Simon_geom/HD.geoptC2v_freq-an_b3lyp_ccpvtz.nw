echo

start molecule

title "Mustard gas, HD"
charge 0

geometry units angstroms print xyz autosym
#  load format xyz HD_konformer81.xyz
  load format xyz HD_geom_Simon.xyz
#  symmetry C2v
end

basis
  * library cc-pvtz
end

dft
  grid fine
  xc b3lyp
  mult 1
end

driver
  tight
  linopt 0
  maxiter 50
  xyz HD_geomSimon_ccpvtz_again
end


task dft optimize
task dft freq 

