echo

start molecule

title "VX"
charge 0

geometry units angstroms print xyz autosym
  load format xyz VX_geom_ccpvdz-034-again.xyz
end

basis
  * library cc-pvtz
end

dft
  grid fine
  xc b3lyp
  mult 1
end

driver
  maxiter 50
  xyz VX_geom_ccpvtz
end


task dft optimize
task dft freq numerical

