echo

start molecule

title "Mustard gas, HD"
charge 0

geometry units angstroms print xyz autosym
  load format xyz HD_geom_ccpvtz.xyz
end

basis
  * library cc-pvtz
end

dft
  grid fine
  xc b3lyp
  mult 1
end

stepper
  maxiter 50
# xyz HD_geomstepper_ccpvtz
end


task dft optimize
task dft freq numerical

