echo

start molecule

title "Mustard gas, HD"
charge 0

geometry units angstroms print xyz autosym
  load format xyz HD_hamid.xyz
end

basis
  * library cc-pvtz
end

dft
  grid fine
  xc b3lyp
  mult 1
end

driver
  maxiter 50
  xyz HD_geom_ccpvtz_fromhamid
end


task dft optimize
task dft freq numerical

