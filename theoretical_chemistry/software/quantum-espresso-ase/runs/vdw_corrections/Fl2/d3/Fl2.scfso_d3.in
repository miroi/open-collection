&CONTROL
  calculation = 'scf'
  pseudo_dir = '../.'
  verbosity = 'high'
/

&SYSTEM
  smearing = 'gauss'
  ecutwfc =  80
  ibrav = 0
  nat = 2
  nosym = .false.
  ntyp = 1
  occupations = 'smearing'
  degauss = 0.02,
  lspinorb = .true.,
  noncolin=.true.,
  vdw_corr = 'grimme-d3'
/

&ELECTRONS
  mixing_beta = 0.3
  diagonalization = 'david' 
  conv_thr =   1.D-9
  electron_maxstep = 100
  startingpot      = "atomic"
  startingwfc      = "atomic+random"

/

