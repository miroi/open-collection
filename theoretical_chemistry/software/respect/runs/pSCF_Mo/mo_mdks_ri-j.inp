pscf:
     geo-units:  angstrom
     geometry:
          Mo   0.000000   0.000000   0.000000

     basis:          dyall-dz
     method:         mdks/pbe
     initmo:         atomic
     nc-model:       gauss
     diis:           on
     maxiterations:  30
     convergence:    1.0e-6

     grid:
          all:  50  adaptive
     peri:
          acceleration:  ri-j
          threshold:  1.0e-8
